const claimRequest = require('claimRequest');
const getRequestPath = require('getRequestPath');
const getRequestBody = require('getRequestBody');
const runContainer = require('runContainer');
const returnResponse = require('returnResponse');
const setResponseHeader = require('setResponseHeader');
const setResponseStatus = require('setResponseStatus');
const getRequestHeader = require('getRequestHeader');
const sendHttpRequest = require('sendHttpRequest');
const encodeUriComponent = require('encodeUriComponent');
const logToConsole = require('logToConsole');
const makeString = require('makeString');
const JSON = require('JSON');

// === Build base URL for Stape Store ===
function getBaseUrl() {
  const id = getRequestHeader('x-gtm-identifier');
  const domain = getRequestHeader('x-gtm-default-domain');
  const key = getRequestHeader('x-gtm-api-key');

  return 'https://' + encodeUriComponent(id) + '.' + encodeUriComponent(domain) +
         '/stape-api/' + encodeUriComponent(key) + '/v1/store';
}

// === Fetch from Stape Store ===
function fetchStapeData(documentId, callback) {
  const url = getBaseUrl() + '/' + encodeUriComponent(documentId);

  sendHttpRequest(url, function(statusCode, headers, body) {
    var data = {};
    if (statusCode === 200 && body && body !== '') {
      data = JSON.parse(body);
    }
    callback(data);
  });
}

// === Flatten while preserving nested structures ===
function flattenPreserving(obj, preserveKeys) {
  var flat = {};
  for (var key in obj) {
    if (preserveKeys.indexOf(key) !== -1) {
      flat[key] = obj[key]; // keep as object
    } else if (typeof obj[key] === 'object' && obj[key] !== null) {
      for (var subKey in obj[key]) {
        flat[subKey] = obj[key][subKey];
      }
    } else {
      flat[key] = obj[key];
    }
  }
  return flat;
}

let isClientUsed = false;
const requestPath = getRequestPath();

if (data.path && !isClientUsed) {
  for (var i = 0; i < data.path.length; i++) {
    if (data.path[i].path === requestPath) {
      runClient();
    }
  }
}

function runClient() {
  isClientUsed = true;
  claimRequest();

  const rawBody = getRequestBody();
  const parsed = JSON.parse(rawBody);

  const fallbackId = parsed.cart_token || parsed.checkout_token;
  const manualId = data.stapeKeyField;
  const documentId = manualId || fallbackId;

  if (!documentId) {
    setResponseStatus(400);
    return returnResponse(JSON.stringify({ error: 'Missing document ID' }));
  }

  fetchStapeData(documentId, function(stapeData) {
    const enriched = flattenPreserving(stapeData, ['client_hints', 'x-sst-system_properties']);

    // — user_data: structured with address as array-like object and _tag_mode
    enriched.user_data = {
      email: parsed.user_data.email,
      phone_number: parsed.user_data.phone,
      address: {
        0: {
          first_name: parsed.user_data.first_name,
          last_name: parsed.user_data.last_name,
          street: parsed.user_data.street,
          city: parsed.user_data.city,
          country: parsed.user_data.country,
          postal_code: parsed.user_data.zip
        }
      },
      _tag_mode: "MANUAL"
    };

    // — ecommerce → top-level
    const ec = parsed.ecommerce || {};
    enriched.transaction_id      = ec.transaction_id;
    enriched.currency            = ec.currency;
    enriched.value               = ec.value;
    enriched.coupon              = ec.coupon;
    enriched.tax                 = ec.tax;
    enriched.shipping            = ec.shipping;
    enriched.discount_amount     = ec.discount_amount ? (ec.discount_amount * 1) >> 0 : ec.discount_amount;

    // *** NEW: precompute total line value for proportional discount allocation ***
    const _itemsArr = ec.items || [];
    const _totalLineValue = _itemsArr.reduce(function (sum, it) {
      var p = it && it.price != null ? (it.price * 1) : 0;
      var q = it && it.quantity != null ? it.quantity : 1;
      return sum + (p * q);
    }, 0);

    // — items: include item_sku, use item_variant ID, and add variant_name
    enriched.items = _itemsArr.map(function(item) {
      // existing per-item discount (if webhook ever provides it)
      var existingDiscount = item.discount;

      // fallback from top-level discount_amount or discount_percentage
      var priceNum = item.price != null ? (item.price * 1) : 0;
      var qty      = item.quantity || 1;
      var computedDiscount = existingDiscount;

      if (computedDiscount != null) {
        computedDiscount = (computedDiscount * 1) >> 0;
      } else if (ec.discount_amount) {
        // allocate proportionally by line value; save as per-unit discount
        var lineValue = priceNum * qty;
        var share = ((ec.discount_amount * 1) * lineValue) / (_totalLineValue || 1);
        computedDiscount = (share / qty) >> 0;
      }

      return {
        item_id:        item.item_variant,
        item_sku:       item.item_sku,
        item_name:      item.item_name,
        item_variant:   item.item_id,
        variant_name:   item.variant_name,
        item_brand:     item.item_brand,
        item_category:  item.item_category,
        price:          item.price ? (item.price * 1) >> 0 : item.price,
        discount:       computedDiscount,
        quantity:       item.quantity,
        coupon:         ec.coupon
      };
    });

    // — top-level quantity = sum of all item quantities
    enriched.quantity = enriched.items
      .reduce((sum, it) => sum + (it.quantity || 0), 0);

    enriched.content_ids = enriched.items.map(function(item) {
      return makeString(item.item_id);
    });

    enriched.event_name = 'purchase';

    runContainer(enriched);

    setResponseStatus(200);
    setResponseHeader('Content-Type', 'application/json');
    return returnResponse(JSON.stringify({ status: 'processed' }));
  });
}


